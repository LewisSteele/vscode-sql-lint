{
  "name": "vscode-sql-lint",
  "displayName": "vscode-sql-lint",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:sql"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "sql-lint configuration",
      "properties": {
        "sql-lint.host": {
          "type": "string",
          "default": null,
          "description": "MySQL database hostname"
        },
        "sql-lint.user": {
          "type": "string",
          "default": null,
          "description": "MySQL database hostname"
        },
        "sql-lint.password": {
          "type": "string",
          "default": null,
          "description": "MySQL database hostname"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/expect": "^24.3.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "chai": "^4.3.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "vscode-test": "^1.5.0"
  }
}
