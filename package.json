{
	"name": "vscode-sql-lint",
	"displayName": "vscode-sql-lint",
	"description": "meh",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Linters"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:php",
		"onLanguage:sql",
		"onLanguage:typescript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/LewisSteele/vscode-sql-lint"
	},
	"main": "./out/extension.js",
	"icon": "images/mysql.png",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "PHP Mess Detector configuration",
			"properties": {
				"sql-lint.host": {
					"type": "string",
					"default": null,
					"description": "MySQL database hostname"
				},
				"sql-lint.user": {
					"type": "string",
					"default": null,
					"description": "MySQL database hostname"
				},
				"sql-lint.password": {
					"type": "string",
					"default": null,
					"description": "MySQL database hostname"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
