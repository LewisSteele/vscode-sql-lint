{
  "name": "vscode-sql-lint",
  "displayName": "vscode-sql-lint",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:sql"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "sql-lint configuration",
      "properties": {
        "sql-lint.host": {
          "type": "string",
          "default": null,
          "description": "MySQL database hostname"
        },
        "sql-lint.user": {
          "type": "string",
          "default": null,
          "description": "MySQL database username"
        },
        "sql-lint.password": {
          "type": "string",
          "default": null,
          "description": "MySQL database password"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "ts-standard",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
